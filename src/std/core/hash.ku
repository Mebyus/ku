fun hash64_djb2(s: []u8) => u64 {
    var h: u64 = 5381; // magic number
    for i = [:s.len] {
        h = ((h << 5) + h) + cast(u64, s.ptr.[i]);
    }
    ret h;
}

fun hash64_sdbm(s: []u8) => u64 {
    var h: u64 = 0;
    for i = [:s.len] {
        h = cast(u64, s.ptr.[i]) + (h << 6) + (h << 16) - h;
    }
    ret h;
}
